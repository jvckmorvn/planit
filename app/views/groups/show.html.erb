<div class="container">
  <div class="row">
    <div class="col-2">
      <%= cl_image_tag @group.picture.key, class: "avatar-bordered" %>
      <h3><%= @group.name %></h3>
      <p>All events</p>
    </div>
    <div class="col-5">
      <p>Calendar</p>
    </div>
    <div class="col-5">
      <p>Map</p>
    </div>
  </div>
  <div class="row">
    <div class="col-2">
      <% @events.each do %>
        <%= render "/events/card", event: event %>
      <% end %>
      <%# render "/events/form", event: @event %>
    </div>
    <div class="col-10">
      <%# render Calendar/Map %>
    </div>
  </div>
</div>
