<div class="col-sm-3">
  <div class="group-card">
    <a href="<%= link_to group_path(group) %>">
      <div class="card-body" style="background-image: url('<%= cl_image_path group.picture.key %>');">
        <h5 class="card-title"><%= group.name %></h5>
        <p class="card-text"><strong><%= group.users.count %></strong> members</p>
      </div>
    </a>
  </div>
</div>
